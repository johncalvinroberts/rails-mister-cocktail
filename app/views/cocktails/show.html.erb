<h1><%= @cocktail.name %></h1>
<%= link_to "add ingredients", new_cocktail_dose_path(@cocktail), class:"btn btn-default" %>

<ul>
<% @cocktail.doses.each do |dose| %>
    <li><%= dose.ingredient.name %>, <%= dose.description %>, <%= link_to "delete", dose_path(dose), method: :delete %></li>
<% end %>
 </ul>
